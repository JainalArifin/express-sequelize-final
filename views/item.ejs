<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/items">items</a>
    <a href="/suppliers">suppliers</a><br><br>
    <a href="/items/add">add item</a>
    <table border="2">
      <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>BRAND</th>
        <th>codeitem</th>
        <th colspan="2">Action</th>
      </tr>
      <% dtItem.forEach(dataItem =>{ %>
        <tr>
          <td><%= dataItem.id %></td>
          <td><%= dataItem.name %></td>
          <td><%= dataItem.brand %></td>
          <td><%= dataItem.codeitem %></td>
          <td>
            <a href="/items/edit/<%= dataItem.id %>">edit</a>
          </td>
          <td>
            <a href="/items/delete/<%= dataItem.id %>">Delete</a>
          </td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
