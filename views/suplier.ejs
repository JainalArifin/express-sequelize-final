<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <a href="/items">items</a>
    <a href="/suppliers">suppliers</a><br><br>
    <a href="/suppliers/add">add suppliers</a>
    <table border="2">
      <tr>
        <th>ID</th>
        <th>name</th>
        <th>kota</th>
        <th>action</th>
      </tr>
      <% dtSupplier.forEach(dataSuppliers=>{ %>
      <tr>
        <td><%= dataSuppliers.id %></td>
        <td><%= dataSuppliers.name %></td>
        <td><%= dataSuppliers.kota %></td>
        <td>
          <a href="/suppliers/edit/<%= dataSuppliers.id %>">Edit</a>
        </td>
        <td>
          <a href="/suppliers/delete/<%= dataSuppliers.id %>">Delete</a>
        </td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
